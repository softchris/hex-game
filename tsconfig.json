{
  "compilerOptions": {
    "target": "es5",
    "module": "es6",
    "moduleResolution": "Node",
    "outDir": "./dist",
    "sourceMap": true,
    "noImplicitAny": false,
    "strict": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true
  }
}